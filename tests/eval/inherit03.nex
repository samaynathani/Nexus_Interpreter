class Super
{
   var x;

   fun init()
   {
      this.x = 0;

      return this;
   }
}

class Sub : Super
{
   fun increment()
   {
      this.x = this.x + 1;
   }
}

var o;

o = new Sub.init();

print o.x;
print ":n";

o.increment();
o.increment();
o.increment();
o.increment();

print o.x;
print ":n";
